<?xml version="1.0" encoding="utf-8"?>

<odoo noupdate="1">

    <record id="config_sendy_url" model="ir.config_parameter">
      <field name="key">sendy_url</field>
      <field name="value">http://your_sendy_instance/</field>
    </record>

    <record id="config_sendy_list" model="ir.config_parameter">
      <field name="key">sendy_list</field>
      <field name="value">list_id_here</field>
    </record>
    <record id="config_sendy_api_key" model="ir.config_parameter">
        <field name="key">sendy_api_key</field>
        <field name="value">api_key_here</field>
    </record>
</odoo>
