<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="stock_return_picking_form_view_ext" model="ir.ui.view">
        <field name="name">stock.return.picking.form.view.ext</field>
        <field name="model">stock.return.picking</field>
        <field name="inherit_id" ref="stock.view_stock_return_picking_form" />
        <field name="arch" type="xml">
            <field name="location_id" position="after">
                <field name="show_return_raw_material" invisible="1"/>
                <field name="is_return_raw_materials" attrs="{'invisible':[('show_return_raw_material', '=', False)]}"/>
            </field>
        </field>
    </record>

</odoo>
